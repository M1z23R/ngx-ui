{
  "name": "ngx-ui",
  "version": "1.0.5",
  "scripts": {
    "ng": "ng",
    "prestart" : "npm run build",
    "start": "ng serve",
    "build": "ng build ngx-ui",
    "build:lib": "ng build ngx-ui --configuration production",
    "watch": "ng build ngx-ui --watch --configuration development",
    "test": "ng test",
    "publish:lib": "cd dist/ngx-ui && npm publish --access public"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
